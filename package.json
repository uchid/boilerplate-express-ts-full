{
  "name": "boilerplate-express-ts-full",
  "version": "1.0.0",
  "main": "app.js",
  "license": "MIT",
  "scripts": {
    "start": "yarn run build:prod && node ./dist/app.bundle.js",
    "start:dev": "yarn run lint && webpack --config webpack.dev.js",
    "start:without-webpack": "NODE_ENV=development ts-node --project tsconfig.ts-node.json ./src/app.ts",
    "build:prod": "yarn run lint && webpack -p --config webpack.prod.js",
    "lint": "tslint --project tsconfig.dev.json --format stylish 'src/**/*.ts'",
    "nodemon": "NODE_ENV=development nodemon",
    "test": "jest --verbose --coverage",
    "db:init": "sequelize db:create",
    "db:generate:migration": "./shell/sequelize/migration-generate.sh",
    "db:generate:seeder": "./shell/sequelize/seeder-generate.sh",
    "db:migrate": "./shell/sequelize/migrate.sh",
    "db:seed": "./shell/sequelize/seed.sh"
  },
  "dependencies": {
    "@types/express": "^4.17.1",
    "@types/moment-timezone": "^0.5.12",
    "@types/sequelize": "^4.28.6",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "inversify": "^5.0.1",
    "moment-timezone": "^0.5.27",
    "mysql2": "^1.7.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.1",
    "sequelize-cli": "^5.5.1",
    "sequelize-typescript": "^1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.21",
    "@types/supertest": "^2.0.8",
    "clean-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.2",
    "nodemon-webpack-plugin": "^4.1.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^3.6.3",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  }
}
